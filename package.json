{
  "name": "taalmeet-app",
  "version": "1.0.0",
  "description": "TaalMeet - A React Native language exchange app with clean architecture",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development npx expo start",
    "start:dev": "NODE_ENV=development npx expo start",
    "start:staging": "NODE_ENV=staging npx expo start",
    "start:prod": "NODE_ENV=production npx expo start",
    "android": "NODE_ENV=development npx expo start --android",
    "android:dev": "NODE_ENV=development npx expo start --android",
    "android:staging": "NODE_ENV=staging npx expo start --android",
    "android:prod": "NODE_ENV=production npx expo start --android",
    "ios": "NODE_ENV=development npx expo start --ios",
    "ios:dev": "NODE_ENV=development npx expo start --ios",
    "ios:staging": "NODE_ENV=staging npx expo start --ios",
    "ios:prod": "NODE_ENV=production npx expo start --ios",
    "web": "NODE_ENV=development npx expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:e2e": "jest __tests__/e2e",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run test",
    "prepare": "husky install"
  },
  "keywords": [
    "react-native",
    "typescript",
    "language-exchange",
    "clean-architecture"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "@supabase/supabase-js": "^2.38.0",
    "expo": "~54.0.0",
    "expo-constants": "^18.0.10",
    "expo-linear-gradient": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.0",
    "react-native-worklets": "^0.5.1",
    "react-native-worklets-core": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@expo/metro-config": "~54.0.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^29.2.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "metro-minify-terser": "^0.83.3",
    "nock": "^14.0.10",
    "prettier": "^3.0.0",
    "react-test-renderer": "19.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=20.19.4",
    "npm": ">=9.0.0"
  }
}
